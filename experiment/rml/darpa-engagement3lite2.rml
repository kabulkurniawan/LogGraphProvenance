    rml:source [
      a carml:Stream ;
    ] ;
    rml:referenceFormulation ql:JSONPath ;
    rml:iterator "$.logEntry" ;
  ] ;

 rr:subjectMap [ rr:template "http://sepses.res/darpa/entity#{fileUUID}";
